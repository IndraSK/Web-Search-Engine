            viewer_iframe_id = '#First-Iframe';
    
            $("#copy_permalink").on('click', function(e) {
            var selectText = 'https://n2t.net/ark:/81984/d3610w20n';
            var input = document.createElement('input');
            input.value = selectText;
            document.body.appendChild(input);
            input.select();
            document.execCommand("copy");
            document.body.removeChild(input);
            
            $(".tooltip-inner").html("Copied");
            
        });
    
            $("#copy_citation").on('click', function(e) {
            var selectText = '1901 graduating class, University of Illinois College of Medicine, College of Physicians
and Surgeons of Chicago – Class Photos, https://n2t.net/ark:/81984/d3610w20n';
            var input = document.createElement('input');
            input.value = selectText;
            document.body.appendChild(input);
            input.select();
            document.execCommand("copy");
            document.body.removeChild(input);

            $(".tooltip-inner").html("Copied");
        });
    
            $("#copy_manifest").on('click', function(e) {
            var selectText = 'https://digital.library.uic.edu/iiif/manifest/ark:/81984/d3610w20n';
            var input = document.createElement('input');
            input.value = selectText;
            document.body.appendChild(input);
            input.select();
            document.execCommand("copy");
            document.body.removeChild(input);

            $(".tooltip-inner").html("Copied");
        });
    



    function afterLoadingIframe(first_image_url) {

            }

    function load_file_information(file_id, data_page){
            $.ajax({
                url: 'https://digital.library.uic.edu/file/get_file_object_data/' + file_id,
                cache: false,
                type: "GET",
                success: function(response) {
                    if (response != "[]") {
                        obj = jQuery.parseJSON(response);

                        $(".file_selected_name").removeClass("d-none");
                        $(".file_selected_name h1").html(obj.object_name);

                        $("#item-info-tab").removeClass("d-none");
                        $("#item-metadata-info").html(obj.html.replace("%permalink_child_value%", "https://n2t.net/ark:/81984/d3610w20n/?search_file=" + file_id + "&search_page=" + (data_page + 1) ));
                        $("#header-list-metadata").removeClass("d-none");

                    } else {
                        $(".file_selected_name").addClass("d-none");
                        $(".file_selected_name h1").html("");

                        $("#item-info-tab").addClass("d-none");
                        $("#item-metadata-info").html("");
                        $("#header-list-metadata").addClass("d-none");
                    }
                },
                error: function(xhr, response) {
                    $(".file_selected_name").addClass("d-none");
                    $(".file_selected_name h1").html("");

                    $("#item-info-tab").addClass("d-none");
                    $("#item-metadata-info").html("");
                    $("#header-list-metadata").addClass("d-none");
                }
            });
    }

    $(document).ready(function() {
        $(".medatada_info_icon").click(function(){
            message = $(this).attr("data-message");
            $('.modal-info .info-text').html(message);
            $('.modal-info').modal("show");
        });
    });
</script>

