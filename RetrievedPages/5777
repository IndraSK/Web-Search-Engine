    <header class="masthead masthead-browse text-white text-center" style="background: linear-gradient(rgba(255, 255, 255, 0.45), rgba(0, 0, 0, 0.71)), url(/hero/765221) no-repeat center center; background-size:cover; padding-top: 10rem !important; padding-bottom: 5rem !important; ">
        <div class="container-fluid">
